{
  "name": "electron-tuto",
  "description": "A Electron 20 Starter Boilerplate with Vue 3, Vue Router 4, Pinia 2, Bulma, Eslint, Axios, Jest 28 and More. Start creating your macOS and Windows apps right now!",
  "version": "1.3.1",
  "private": false,
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "vue",
    "axios",
    "pinia",
    "bulma",
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morellexf26/electron-tuto.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "babel-jest": "^28.1.3",
    "@sucrase/jest-plugin": "^2.2.1",
    "@vue/vue3-jest": "^28.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "ts-jest": "^28.0.7",
    "jest": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "vue-router-mock": "^0.1.8",
    "@pinia/testing": "^0.0.13",
    "@vue/test-utils": "^2.0.2",
    "@babel/eslint-parser": "^7.18.9",
    "@electron/remote": "^2.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "bulma": "^0.9.4",
    "axios": "^0.27.2",
    "core-js": "^3.24.1",
    "electron": "^20.0.1",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "howler": "^2.2.3",
    "identity-obj-proxy": "^3.0.0",
    "pinia": "^2.0.17",
    "pinia-plugin-persist": "^1.0.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-router": "^4.1.3"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "^23.0.3"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
